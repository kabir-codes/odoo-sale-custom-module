<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="sale_order_line_wizard_form" model="ir.ui.view">
        <field name="name">sale.order.line.wizard.form</field>
        <field name="model">sale.order.form</field>
        <field name="arch" type="xml">
            <form class="form_sale_demo">
                <sheet>
                    <group name="full-name" col="4">
                        <field name="first_name"/>
                        <field name="last_name"/>
                        <field name="date_of_birth"/>
                        <field name="select_occupancy" widget="selection"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
